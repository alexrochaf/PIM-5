@{
    Page.Title = "The Host - Hospeds";
    Layout = "../_Layout.cshtml";

    var conexao = WebMatrix.Data.Database.Open("DB");

    var hospedes = conexao.Query("select * from HOSPEDES");

    conexao.Close();

}
<h1>Cadastro Hospedes</h1>
<form id="formCadastroHospede" role="form" action="" method="post">
    <div class="form-group col-lg-3">
        <label>Código</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="CLI025" disabled="1">
    </div>
    <div class="form-group col-lg-9">
        <label>Nome Completo</label>
        <input id="nome" class="form-control">
        <p class="help-block">Ex. João Silveira Soares</p>
    </div>
    <div class="form-group col-lg-2">
        <label>CEP</label>
        <input id="cep" type="cep" class="form-control" onblur="pesquisacep(this.value)" />
    </div>
    <div class="form-group col-lg-5">
        <label>Rua</label>
        <input name="rua" type="text" id="rua" class="form-control">
    </div>
    <div class="form-group col-lg-2">
        <label>Número</label>
        <input type="text" class="form-control">
    </div>
    <div class="form-group col-lg-5">
        <label>Bairro</label>
        <input name="bairro" type="text" id="bairro" class="form-control">
    </div>
    <div class="form-group col-lg-10">
        <label>Cidade</label>
        <input name="cidade" type="text" id="cidade" class="form-control">
    </div>
    <div class="form-group col-lg-2">
        <label>UF</label>
        <input name="uf" type="text" id="uf" class="form-control">
    </div>
    <div class="form-group col-lg-6">
        <label>CPF</label>
        <input id="cpf" class="form-control" type="text" name="cpf" pattern="\d{3}\.\d{3}\.\d{3}-\d{2}" title="Digite o CPF no formato 000.000.000-00">
        <p class="help-block">Ex. 999.999.999-99</p>
    </div>
    <div class="form-group col-lg-4">
        <label>Telefone</label>
        <input id="telefone" class="form-control" type="text" name="telefone" pattern="\d{1}\-\d{4}\-\d{4}" title="Digite o telefone no formato 9-9999-9999">
        <p class="help-block">Ex. 9-9999-9999</p>
    </div>
    <div class="form-group col-lg-3">
        <label>Data de Nascimento</label>
        <input id="cpf" class="form-control" type="text" name="cpf" pattern="\d{2}\/\d{2}\/\d{4}" title="Digite a data de nascimento no formato 00/00/0000">
        <p class="help-block">Ex. 01/01/1900</p>
    </div>
    <!-- BOTÕES ENDEREÇO / CONTATO    = fazer script para mudar Nav Tab ativa =  -->
    <div class="col-lg-12">
        <button type="submit" class="btn btn-success">Próximo</button>
        <button type="reset" class="btn btn-warning">Limpar</button>
    </div>
</form>